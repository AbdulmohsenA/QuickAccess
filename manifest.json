{
    "manifest_version": 3,
    "name": "Quick Access",
    "description": "Quick access to sites you add",
    "version": "1.10.4",
    "permissions": [
        "tabs",
        "storage",
        "contextMenus"
      ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons" : {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}